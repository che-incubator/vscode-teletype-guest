{
	"name": "vscode-teletype",
	"displayName": "vscode-teletype",
	"description": "VS Code Teletype extension for Creating CoEditing skeleton for Eclipse Che and Theia",
	"types": "dist/index.d.ts",
	"publisher": "redhat",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rijul5/vscode-teletype.git"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.join-portal",
		"onCommand:teletypeCoding.start",
		"onCommand:teletypeCoding.doRefactor",
		"onWebviewPanel:teletypeCoding"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.join-portal",
				"title": "Join Portal"
			},
			{
				"command": "teletypeCoding.start",
				"title": "Start cat coding session",
				"category": "Teletype Coding"
			},
			{
				"command": "teletypeCoding.doRefactor",
				"title": "Do some refactoring",
				"category": "Teletype Coding"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"package": "vsce package"
	},
	"devDependencies": {
		"@mapbox/node-pre-gyp": "^1.0.5",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.17.28",
		"@types/vscode": "^1.56.0",
		"@types/vscode-webview": "^1.57.0",
		"awesome-typescript-loader": "^5.2.1",
		"deep-equal": "^1.1.1",
		"source-map-loader": "^0.2.4",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7",
		"vsce": "~1.71.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"@atom/teletype-client": "github:Rijul5/teletype-client",
		"google-protobuf": "^3.12.4",
		"mkdirp-promise": "^5.0.1",
		"node-fetch": "^2.6.0",
		"vscode": "^1.1.37",
		"wrtc": "^0.4.6"
	}
}
